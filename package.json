  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.1",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.8",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.0",
